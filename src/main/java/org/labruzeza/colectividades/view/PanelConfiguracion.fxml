<?import javafx.scene.layout.*?>
<?import org.javafx.controls.customs.*?>

<fx:root type="javafx.scene.layout.BorderPane" xmlns:fx="http://javafx.com/fxml">
	<center>
	 <BorderPane BorderPane.alignment="CENTER" >
	  	<top>
            <VBox fx:id="vBoxMsg"  BorderPane.alignment="CENTER" />
        </top>
	 	<center>
			<GridPane vgap="5" hgap="5">
				<padding><Insets top="25" right="25" bottom="10" left="25"/></padding>   
				<Label text="%Configuracion.panel.field.idconfiguracion" GridPane.columnIndex="0" GridPane.rowIndex="1"/>   
				<NumberField fx:id="txtidconfiguracion" maxWidth="100" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
				<Label text="%Configuracion.panel.field.nrocaja" GridPane.columnIndex="0" GridPane.rowIndex="2"/>   
				<NumberField fx:id="txtnrocaja" maxWidth="100" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
				<Label text="%Configuracion.panel.field.fecha" GridPane.columnIndex="0" GridPane.rowIndex="3"/>   
 				<DatePicker fx:id="dprfecha" GridPane.columnIndex="1" GridPane.rowIndex="3"/>       
			</GridPane>
       </center>
	 </BorderPane>	
	</center>
</fx:root> 